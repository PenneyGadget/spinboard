<div class="container-fluid text-center">
  <h4 class="page-header">Welcome back! Please log in:</h4>
    <div class="form-text">
      <%= form_for(:session, url: login_path) do |f| %>
        <%= f.label :email %>:
        <br>
        <%= f.text_field :email %>
        <br><br>
        <%= f.label :password %>:
        <br>
        <%= f.password_field :password %>
        <br><br>
        <%= f.submit "Log In", class:"submit-btn" %>
      <% end %>
    </div>
</div>
